<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="main.java.com.cart.bookmark.Bookmark" table="bookmark">
        <id name="id" type="int" unsaved-value="null">
            <column name="bookmark_id" sql-type="int(11)" not-null="true" />
            <generator class="assigned" />
        </id>

        <property name="folderId">
            <column name="folder_id" sql-type="int(11)" not-null="true" />
        </property>
        <property name="name">
            <column name="bookmark_name" length="100" not-null="true" />
        </property>
        <property name="url">
            <column name="bookmark_url" not-null="true" />
        </property>

    </class>
</hibernate-mapping>